{
  "name": "boilerplate-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "jest": "NODE_ENV=test jest --config=jest/jest.config.ts --runInBand --forceExit",
    "migrate": "npx prisma migrate dev",
    "dev:migrate": "npm run migrate && npm run dev",
    "migrate:local": "docker exec -it app npm run migrate",
    "up": "docker-compose -f docker-compose.yml up -d && docker-compose logs -f",
    "down": "docker-compose -f docker-compose.yml down",
    "up:test": "docker-compose -f docker-compose.test.yml up -d && docker-compose logs -f",
    "down:test": "docker-compose -f docker-compose.test.yml down",
    "jest:local": "docker exec -it app-test npm run jest",
    "cypress:open": "cypress open --config-file cypress/cypress.config.ts",
    "cypress": "cypress run --config-file cypress/cypress.config.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.9.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@supabase/supabase-js": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/formidable": "^2.0.5",
    "@types/validator": "^13.7.11",
    "bcrypt": "^5.1.0",
    "cypress": "^12.4.0",
    "formidable": "^2.1.1",
    "next": "13.1.2",
    "next-auth": "^4.18.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.42.1",
    "react-redux": "^8.0.5",
    "typescript": "4.9.4",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "node-mocks-http": "^1.12.1",
    "prisma": "^4.9.0",
    "ts-jest": "^29.0.5"
  }
}
